{
  "name": "vite-plugin-pwa",
  "description": "Zero-config PWA for Vite",
  "version": "0.3.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "antfu <anthonyfu117@hotmail.com>",
  "repository": {
    "type": "git",
