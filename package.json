    "@types/debug": "^4.1.5",
    "@types/workbox-build": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "eslint": "^7.17.0",
    "rollup": "^2.35.1",
    "tsup": "^3.11.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.3"
